             skos:altLabel "hiearchical"@en ,
                           "is-a"@en ;
             
             skos:prefLabel "subsumption"@en .



###  http://kbpedia.org/ontologies/kko#SuperTypes

:SuperTypes rdf:type owl:Class ;
            
            skos:scopeNote "A Thirdness"@en ;
            
